{
	"name": "sveltekit-tanstack-electric",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "drizzle-kit generate && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"backend:up": "PROJECT_NAME=sveltekit-tanstack-electric pnpm run example-backend:up && pnpm run migrate",
		"backend:down": "PROJECT_NAME=sveltekit-tanstack-electric pnpm run example-backend:down",
		"backend:clear": "pnpm run  backend:down && pnpm run  backend:up",
		"example-backend:up": "dotenv -e .env.dev -- docker compose -f ./.support/docker-compose.yml up -d",
		"example-backend:down": "dotenv  -e .env.dev -- docker compose -f .support/docker-compose.yml down --volumes",
		"generate": "drizzle-kit generate",
		"migrate": "drizzle-kit migrate",
		"prepare": "husky",
		"studio": "drizzle-kit studio",
		"proxy": "pnpx @radically-straightforward/caddy run --config caddy.config --adapter caddyfile"
	},
	"devDependencies": {
		"@flydotio/dockerfile": "^0.5.9",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.17.3",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/typography": "^0.5.16",
		"@tanstack/svelte-query-devtools": "^5.66.11",
		"@types/eslint": "^9.6.1",
		"@types/pg": "^8.11.11",
		"@types/uuid": "^10.0.0",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^1.3.5",
		"clsx": "^2.1.1",
		"dotenv": "^16.4.7",
		"dotenv-cli": "^7.4.4",
		"drizzle-kit": "^0.26.2",
		"eslint": "^9.21.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^3.0.2",
		"formsnap": "^2.0.0",
		"globals": "^15.15.0",
		"husky": "^9.1.7",
		"lucide-svelte": "^0.452.0",
		"mode-watcher": "^0.4.1",
		"prettier": "^3.5.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.20.5",
		"svelte-check": "^4.1.4",
		"svelte-sonner": "^0.3.28",
		"sveltekit-superforms": "^2.23.1",
		"tailwind-merge": "^2.6.0",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "^3.4.17",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.25.0",
		"vite": "^6.2.0",
		"zod": "^3.24.2"
	},
	"type": "module",
	"dependencies": {
		"@electric-sql/client": "^0.8.0",
		"@emoji-mart/data": "^1.2.1",
		"@radically-straightforward/caddy": "^2.0.5",
		"@tanstack/svelte-query": "^5.66.11",
		"drizzle-orm": "^0.35.3",
		"drizzle-zod": "^0.5.1",
		"emoji-mart": "^5.6.0",
		"postgres": "^3.4.5",
		"svelte-electricsql": "^0.1.2",
		"uuid": "^10.0.0"
	}
}
