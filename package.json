{
	"name": "sveltekit-tanstack-electric",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"backend:up": "PROJECT_NAME=sveltekit-tanstack-electric bun run example-backend:up && bun run db:migrate",
		"example-backend:up": "dotenv -e .env.dev -- docker compose -f ./.support/docker-compose.yml up -d",
		"example-backend:down": "dotenv  -e .env.dev -- docker compose -f .support/docker-compose.yml down --volumes",
		"backend:down": "PROJECT_NAME=sveltekit-tanstack-electric bun run example-backend:down",
		"db:migrate": "dotenv -e .env.dev -- bunx pg-migrations apply --directory ./db/migrations"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/typography": "^0.5.14",
		"@tanstack/svelte-query-devtools": "^5.58.0",
		"@types/eslint": "^9.6.0",
		"@types/pg": "^8.11.10",
		"@types/uuid": "^10.0.0",
		"autoprefixer": "^10.4.20",
		"dotenv": "^16.4.5",
		"dotenv-cli": "^7.4.2",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"svelte": "^4.2.7",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.9",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@electric-sql/client": "^0.6.0",
		"@tanstack/svelte-query": "^5.56.2",
		"clsx": "^2.1.1",
		"tailwind-merge": "^2.5.2",
		"tailwind-variants": "^0.2.1",
		"uuid": "^10.0.0"
	}
}
